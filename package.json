{
    "name": "ulbifront",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start": "webpack serve --env port=3000",
        "build:prod": "webpack --env mode=production",
        "build:dev": "webpack --env mode=development",
        "eslint": "eslint \"**/*.{ts,tsx}\"",
        "eslint:fix": "eslint --fix \"**/*.{ts,tsx}\"",
        "stylelint": "stylelint \"**/*.{css,scss,sass}\"",
        "stylelint:fix": "stylelint --fix \"**/*.{css,scss,sass}\"",
        "format": "prettier --write \"**/*.{js,jsx,mjs,ts,tsx,mts}\"",
        "format:check": "prettier --check \"**/*.{js,jsx,mjs,ts,tsx,mts}\"",
        "unit": "jest --config ./config/jest/jest.config.ts",
        "unit:watch": "jest --watch --config ./config/jest/jest.config.ts",
        "unit:coverage": "jest --coverage --config ./config/jest/jest.config.ts",
        "loki": "npx loki test",
        "loki:ok": "npx loki approve",
        "loki:report": "npm run loki:json && npm run loki:html",
        "loki:json": "node scripts/generate-visual-json-report.js",
        "loki:html": "reg-cli --from .loki/report.json --report .loki/report.html",
        "loki:ci": "npx loki --requireReference --reactUri file:./storybook-static",
        "storybook": "start-storybook -p 6006 -c ./config/storybook",
        "storybook:build": "build-storybook -c ./config/storybook"
    },
    "keywords": [],
    "author": "Stupishin Daniil",
    "license": "ISC",
